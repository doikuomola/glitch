generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Todo{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  todo String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}